  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@tsconfig/node-lts": "^24.0.0",
    "@types/node": "^24.10.4",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jiti": "^2.6.1",
    "moleculer": "^0.14.35",
    "pkgroll": "^2.21.4",
    "prettier": "^3.7.4",
    "semantic-release": "^25.0.3",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.51.0"
