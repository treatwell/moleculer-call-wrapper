  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@tsconfig/node-lts": "^22.0.2",
    "@types/node": "^22.17.2",
    "eslint": "^9.34.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jiti": "^2.5.1",
    "moleculer": "^0.14.33",
    "pkgroll": "^2.15.3",
    "prettier": "^3.6.2",
    "semantic-release": "^24.2.7",
    "typescript": "~5.9.2",
