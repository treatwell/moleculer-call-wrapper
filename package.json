{
  "name": "@treatwell/moleculer-call-wrapper",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/treatwell/moleculer-call-wrapper"
  },
  "version": "0.0.0",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
